@page
@model DuelSysWeb.Pages.LogInModel
@{
     ViewData["Title"] = "Login";
}
@section scripts{
    <partial name="_ValidationScriptsPartial" />
}

<div class="text-center">
    <form class="" method="post">
    <h1>Log in</h1>
   
    <input  placeholder="Email" type="text" asp-for="@Model.Email" />
    
    <input placeholder="Password" type="text" asp-for="@Model.Password"  />
    <input type="submit" name="" value="Login" />
    <tr>Remember me:</tr>
  <input type="checkbox" asp-for="Checkedbox" />
    <div>@ViewData["Message"]</div>
     <span asp-validation-for="Email"></span>
     <span asp-validation-for="Password"></span>
</form>
</div>